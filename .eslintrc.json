{
    "root": true,
    "env": {
        "browser": true,
        "node": true,
        "cypress/globals": true
    },
    "extends": ["airbnb", "eslint:recommended", "plugin:cypress/recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["cypress"],
    "rules": {
        "indent": ["error", 4]
    }
}
